{
  "name": "mern-testing-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install-all": "npm install",
    "test": "jest --runInBand",
    "test:unit": "jest --selectProjects client --runInBand",
    "test:integration": "jest --selectProjects server --runInBand",
    "test:server:unit": "jest --selectProjects server --testPathPattern server/tests/unit --runInBand",
    "test:e2e": "jest --selectProjects server --testPathPattern server/tests/e2e --runInBand",
    "setup-test-db": "node server/scripts/setupTestDb.js"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "babel-jest": "^29.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "jest-environment-node": "^29.0.0",
    "mongodb-memory-server": "^8.12.0",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "mongoose": "^7.0.0",
    "prop-types": "^15.8.1",
    "winston": "^3.10.0"
  }
}
